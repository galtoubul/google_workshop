[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=galtoubul
WorkingDirectory=/home/galtoubul/
ExecStart=/usr/local/bin/gunicorn -b 127.0.0.1:5000 --workers=3 --timeout 200 --chdir /home/galtoubul/backend/web_server/ api:app
Restart=always

[Install]
WantedBy=multi-user.target

