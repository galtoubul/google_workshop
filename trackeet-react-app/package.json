{
  "name": "trackeet-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.10",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/lab": "^5.0.0-alpha.44",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.1.0",
    "@mui/styles": "^5.2.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^17.0.34",
    "@types/react-dom": "latest",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "date-fns": "^2.26.0",
    "firebase": "^9.5.0",
    "framer-motion": "^4.1.17",
    "i18next": "^21.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "npmlog": "^6.0.0",
    "pseudomap": "^1.0.2",
    "react": "^17.0.2",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-google-login": "^5.2.2",
    "react-i18next": "^11.13.0",
    "react-icons": "^4.3.1",
    "react-notifications": "^1.7.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.1",
    "react-scroll": "^1.8.4",
    "react-scroll-wheel-handler": "^2.1.0",
    "react-trello": "^2.2.11",
    "react-use-googlelogin": "^0.12.5",
    "sass": "^1.43.4",
    "true-case-path": "^2.2.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "web-vitals": "^1.1.2",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/styles/css --watch",
    "dev": "parcel src/index.html",
    "start": "react-scripts start",
    "start-api": "cd ../backend/web-server && flask run --no-debugger",
    "start-api-win": "cd ../backend/web-server && python -m flask run --no-debugger",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-pretty-strict",
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-pretty-strict": "8.1.1core-js",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^1.12.3",
    "prettier": "^2.4.1"
  },
  "proxy": "http://localhost:5000"
}
