{
  "name": "trackeet-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.10",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "latest",
    "sass": "^1.43.4",
    "@emotion/styled": "latest",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/lab": "^5.0.0-alpha.44",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "latest",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "date-fns": "^2.25.0",
    "framer-motion": "^4.1.17",
    "i18next": "^21.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.13.0",
    "react-icons": "^4.3.1",
    "react-scripts": "4.0.3",
    "react-trello": "^2.2.11",
    "web-vitals": "^1.1.2",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/styles/css --watch",
    "dev": "parcel src/index.html",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-pretty-strict",
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.163",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-pretty-strict": "8.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^1.12.3",
    "prettier": "^2.4.1"
  }
}
