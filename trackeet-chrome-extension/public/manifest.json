{
  "name": "Trackeet",
  "description": "Trackeet.co - A Website That Makes Tracking Online Orders Easier! This tool is made for helping our users to conveniently scan their online orders, with a minimal effort!",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "tabs","scripting", "identity", "identity.email"
  ],
  "key": "",
  "oauth2": {
    "client_id": "118024983796-ma81u5vqau2f04j108cs09tan9lb4te7.apps.googleusercontent.com",
    "scopes": ["profile email"]
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "keet.png",
    "48": "keet.png",
    "128": "keet.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Trackeet Assistant"
  }
}

