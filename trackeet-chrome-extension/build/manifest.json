{
  "name": "Trackeet",
  "description": "Trackeet tool",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs","scripting", "identity", "identity.email"
  ],
  "key": "",
  "host_permissions": ["<all_urls>","https://www.amazon.com/*"],
  "icons": {
    "16": "keet.png",
    "48": "keet.png",
    "128": "keet.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Trackeet tool"
  }
}